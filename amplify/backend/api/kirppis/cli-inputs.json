{
  "version": 1,
  "serviceConfiguration": {
    "apiName": "kirppis",
    "serviceName": "AppSync",
    "defaultAuthType": {
      "mode": "AWS_IAM"
    },
    "additionalAuthTypes": [
      {
        "mode": "AMAZON_COGNITO_USER_POOLS",
        "cognitoUserPoolId": "authkirppis516e92a5"
      },
      {
        "mode": "AWS_LAMBDA",
        "lambdaFunction": "kirpisUserProfile",
        "ttlSeconds": "300"
      }
    ],
    "conflictResolution": {
      "defaultResolutionStrategy": {
        "type": "AUTOMERGE"
      }
    }
  }
}